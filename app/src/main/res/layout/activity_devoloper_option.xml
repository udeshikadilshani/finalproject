<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".devoloper_option">

    <Button
        android:id="@+id/logOut"
        android:layout_width="356dp"
        android:layout_height="72dp"
        android:layout_marginTop="10dp"
        android:text="logout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/deletedecline" />

    <Button
        android:id="@+id/approvede"
        android:layout_width="356dp"
        android:layout_height="72dp"
        android:layout_marginTop="10dp"
        android:text="Approve"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pending" />

    <Button
        android:id="@+id/pending"
        android:layout_width="356dp"
        android:layout_height="72dp"
        android:layout_marginTop="10dp"
        android:text="Pending "
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/my_ads" />

    <Button
        android:id="@+id/my_ads"
        android:layout_width="356dp"
        android:layout_height="72dp"
        android:layout_marginTop="10dp"
        android:text="Viwe My Ads"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/add2" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="87dp"
        android:layout_height="62dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logomarry"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/deletedecline"
        android:layout_width="356dp"
        android:layout_height="72dp"
        android:layout_marginTop="10dp"
        android:text="Delete and Decline"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/approvede" />

    <Button
        android:id="@+id/add2"
        android:layout_width="356dp"
        android:layout_height="72dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="28dp"
        android:text="ADD Addwateesment"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />
</androidx.constraintlayout.widget.ConstraintLayout>